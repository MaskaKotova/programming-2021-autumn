struct Animal{
       string type;
       string name;
       int age;
       
       Animal(string _type, string _name, int _age)
       {
       type = _type;
       name = _name;
       age = _age;
       }
       
       Animal(string type(type), string name(name), int age(age))
       {
       type = type(type);
       name = name(name);
       age = age(age);
       }
       
       ~Animal()
       {
       delete [] type;
       delete [] name;
       delete [] age;
       }
}
void printAnimal(Animal)
{
std::cout << type << name << age << std::endl;

#include<iostream>
using namespace std;
int main (int argc, char* argv[])
{
   int x = 0;
   int y = 0;
   cin >> x;
   cin >> y;
   for (int i = 1; i <= 8; ++1)
   {
      for (int g = 1; g <= 8; ++g)
      {
         if ((8 - i + 1 == y) == (g == x))
         {
            cout << 0;
            cout << " ";
         }
         else if (((8 - i + 1 - y) == (g - x)) || ((8 - i + 1 - y) == -(g - x)))
         {
            cout << 1;
            cout << " ";
         }
         else
         {
            cout << 2;
            cout << " ";
         }
      }
      cout << endl;
   }
   
   return EXIT_SUCCESS;
}
            
         
